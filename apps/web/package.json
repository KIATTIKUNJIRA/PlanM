{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev-start.mjs",
    "dev:clean": "rimraf .next && kill-port 3000 3001 && node scripts/dev-start.mjs",
    "prebuild": "node scripts/prevent-page-tests.cjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --config vitest.config.mts",
    "test:e2e": "pnpm run e2e",
    "e2e": "pnpm exec playwright test -c ./playwright.config.ts",
    "e2e:headed": "pnpm exec playwright test -c ./playwright.config.ts --headed",
    "e2e:ci": "pnpm exec playwright test -c ./playwright.config.ts --reporter=line",
    "e2e:open": "pnpm exec playwright show-report",
    "pre-e2e": "pnpm exec playwright install --with-deps"
  },
  "dependencies": {
    "@eonasdan/tempus-dominus": "^6.10.4",
    "@geoman-io/leaflet-geoman-free": "^2.18.3",
    "@react-leaflet/core": "2.1.0",
    "@supabase/supabase-js": "^2.38.4",
    "@turf/turf": "^7.2.0",
    "fuse.js": "^7.1.0",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "lucide-react": "^0.441.0",
    "mapbox-gl": "^2.15.0",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "4.2.1",
    "react-leaflet-draw": "^0.21.0",
    "react-map-gl": "^7.1.6",
    "swr": "^2.3.6",
    "ts-types": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.20",
    "@types/leaflet-draw": "^1.0.13",
    "@types/mapbox-gl": "^2.7.15",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10",
    "detect-port": "^2.1.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "jsdom": "^26.1.0",
    "kill-port": "^2.0.1",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3",
    "typescript": "^5",
    "vitest": "^1.6.1"
  }
}